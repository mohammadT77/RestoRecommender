      "cell_type": "code",
      "execution_count": 26,
      "metadata": {
        "id": "ZY6Ku6Eboijj"
      },
      "outputs": [],
      "source": [
        "vector_db = get_vector_database(documents, embedding_model)"
      ]
    },
